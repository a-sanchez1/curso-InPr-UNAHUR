procedure PosicionarPara2doCuadrado() {
  IrAlBorde(Este)
  Mover(Oeste)
  Mover(Oeste)
}


program {
  DibujarCuadradoRojo()
  PosicionarPara2doCuadrado()
  DibujarCuadradoRojo()
}