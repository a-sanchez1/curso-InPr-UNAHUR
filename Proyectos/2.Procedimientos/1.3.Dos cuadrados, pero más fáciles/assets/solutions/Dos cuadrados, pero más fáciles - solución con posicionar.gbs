procedure DibujarCuadradoRojo() {
  Poner(Rojo)
  Mover(Este)
  Poner(Rojo)
  Mover(Este)
  Poner(Rojo)
  Mover(Norte)
  Poner(Rojo)
  Mover(Norte)
  Poner(Rojo)
  Mover(Oeste)
  Poner(Rojo)
  Mover(Oeste)
  Poner(Rojo)
  Mover(Sur)
  Poner(Rojo)
  Mover(Sur)
}

//
// Describe el procedimiento...
