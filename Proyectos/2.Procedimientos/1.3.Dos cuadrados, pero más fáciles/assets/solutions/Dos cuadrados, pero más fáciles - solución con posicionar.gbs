program {
  DibujarCuadradoRojo()
  PosicionarPara2doCuadrado()
  DibujarCuadradoRojo()
}

procedure PosicionarPara2doCuadrado() {
  IrAlBorde(Este)
  Mover(Oeste)
  Mover(Oeste)
}

procedure DibujarCuadradoRojo() {
  Poner(Rojo)
  Mover(Este)
  Poner(Rojo)
  Mover(Este)
  Poner(Rojo)
  Mover(Norte)
  Poner(Rojo)
  Mover(Norte)
  Poner(Rojo)
  Mover(Oeste)
  Poner(Rojo)
  Mover(Oeste)
  Poner(Rojo)
  Mover(Sur)
  Poner(Rojo)
  Mover(Sur)
}
