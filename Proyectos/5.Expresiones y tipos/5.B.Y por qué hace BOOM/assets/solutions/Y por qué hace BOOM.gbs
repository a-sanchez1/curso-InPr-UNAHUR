program {
  DibujarCuadrado()
}

procedure DibujarCuadrado() {
  DibujarLíneaAlNorte()
  DibujarLíneaAlEste()
  DibujarLíneaAlSur()
  DibujarLíneaAlOeste()
}

procedure DibujarLíneaAlNorte() {
  repeat(4) {
    Poner(Verde)
    Mover(Norte)
  }
}

procedure DibujarLíneaAlEste() {
  repeat(4) {
    Poner(Verde)
    Mover(Este)
  }
}

procedure DibujarLíneaAlSur() {
  repeat(4) {
    Poner(Verde)
    Mover(Sur)
  }
}

procedure DibujarLíneaAlOeste() {
  repeat(4) {
    Poner(Verde)
    Mover(Oeste)
  }
}
